# gpcplot base style
savefig.dpi: 600
pdf.fonttype: 42
ps.fonttype: 42
svg.fonttype: none
font.size: 11
axes.labelsize: 11
axes.titlesize: 11
legend.fontsize: 10
xtick.labelsize: 11
ytick.labelsize: 11
axes.linewidth: 0.8
xtick.major.size: 4
ytick.major.size: 4
xtick.direction: in
ytick.direction: in
lines.linestyle: solid
lines.dash_capstyle: round
mathtext.fontset: dejavusans
mathtext.default: regular
